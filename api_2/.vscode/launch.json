  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch .NET 5 App",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net5.0/api.dll",
      "launchBrowser": {
        "enabled": false
      },
      "env": {
        "ASPNETCORE_URLS": "http://localhost:5000",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
    }

  ]
}